---
import type { HTMLAttributes } from 'astro/types'

interface Props extends HTMLAttributes<'svg'> {
  size?: number
}

const { props } = Astro
---

<svg width={props.size} viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" {...props}>
  <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
  <g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g>
  <g id="SVGRepo_iconCarrier">
    <path d="M4 12.6111L8.92308 17.5L20 6.5" stroke="#ffff53" stroke-width="1.152" stroke-linecap="round" stroke-linejoin="round"></path>
  </g>
</svg>
